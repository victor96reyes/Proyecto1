<p id="notice"><%= notice %></p>

<div style="display: inline-block; text-align: right; width: 100%">
<%= form_tag vehiculos_path, :method => 'get' do %>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag t("Search"), :name => nil %>
<% end %>
</div>

<h1>Vehiculos</h1>

<table>
  <thead>
    <tr>
      <th>Motor  </th>
      <th>Marca</th>
      <th>Modelo</th>
      <th>Cantidad de Pasajeros</th>
      <th>AÃ±o de Fabricacion</th>
      <th>Foto</th>
      <th>Cilindraje</th>
      <th>Color</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehiculos.each do |vehiculo| %>
      <tr>
        <td><%= vehiculo.motor %></td>
        <td><%= vehiculo.marca %></td>
        <td><%= vehiculo.modelo %></td>
        <td><%= vehiculo.cantpasj %></td>
        <td><%= vehiculo.anyofabric %></td>
        <td><%= vehiculo.foto %></td>
        <td><%= vehiculo.cilindraje %></td>
        <td><%= vehiculo.color %></td>
        <td><%= link_to t('Show'), vehiculo %></td>
        <td><%= link_to t('Edit'), edit_vehiculo_path(vehiculo) %></td>
        <td><%= link_to t('Destroy'), vehiculo, method: :delete, data: { confirm: t('Are you sure?') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

